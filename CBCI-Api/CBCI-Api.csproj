<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <LangVersion>latest</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <DebugType>embedded</DebugType>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <PackageId>Omega3065.ComponentBasedCustomItemAPI</PackageId>
    <Version>0.1.5</Version>
    <Authors>OMEGA3065</Authors>
    <IsPackable>true</IsPackable>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/OMEGA3065/CBCI-API/</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="BepInEx.AssemblyPublicizer.MSBuild" Version="0.4.2" PrivateAssets="all" />

    <!-- Publicize directly when referencing -->
    <Reference Include="..\..\References\Assembly-CSharp.dll" Publicize="true" Private="false" Pack="false" />
    <Reference Include="..\..\References\CommandSystem.Core.dll">
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="..\..\TestPlugin\server\SL\LabAPI\plugins\global\RaCustomMenu.dll">
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="..\..\TestPlugin\server\SL\LabAPI\plugins\global\0Harmony.dll">
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="MEC">
      <HintPath>..\..\References\Assembly-CSharp-firstpass.dll</HintPath>
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="UnityEngine.CoreModule">
      <HintPath>..\..\References\UnityEngine.CoreModule.dll</HintPath>
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="Mirror">
      <HintPath>..\..\References\Mirror.dll</HintPath>
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="Mono.Security">
      <HintPath>..\..\References\Mono.Security.dll</HintPath>
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="UnityEngine.PhysicsModule">
      <HintPath>..\..\References\UnityEngine.PhysicsModule.dll</HintPath>
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="NorthwoodLib">
      <HintPath>..\..\References\NorthwoodLib.dll</HintPath>
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <Reference Include="ProjectMER">
      <HintPath>..\..\CustomReferences\ProjectMER.dll</HintPath>
      <Private>false</Private>
      <Pack>false</Pack>
    </Reference>
    <None Include="..\LICENSE" Pack="true" PackagePath="" />
    <None Include="..\README.md" Pack="true" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Northwood.LabAPI" Version="1.1.4" />
  </ItemGroup>
</Project>
